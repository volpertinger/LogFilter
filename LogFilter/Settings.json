{
  "inputPath": "Path\To\logfile.json",
  "actions": {
    "writeFilteredResult": {
      "enabled": true,
      "outputPath": "Path\To\filtered_logs.json"
    },
    "collectUniqueEventFields": {
      "enabled": false,
      "fields": [ "Process_Name", "PID" ],
      "outputPath": "Path\To\unique_events_fields.json"
    },
    "collectUniqueProcessFields": {
      "enabled": false,
      "fields": [ "ProcessName", "Version" ],
      "outputPath": "Path\To\unique_process_fields.json"
    },
    "malwareDetection": {
      "enabled": true,
      "programPath": "Path\To\compiled\DetectMalware"
    }
  },
  "writeOptions": {
    "console": false,
    "file": true
  },
  "eventFilter": {
    "enabled": false,
    "operationFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "pidFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "processIndexFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "processNameFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "pathFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "resultFilter": {
      "enabled": false,
      "include": true,
      "data": []
    }
  },
  "processFilter": {
    "enabled": false,
    "withModules": false,
    "commandLineFilter": {
      "enabled": false,
      "include": true,
      "data": [ "" ]
    },
    "companyNameFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "isVirtualizedFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "processIdFilter": {
      "enabled": false,
      "include": true,
      "data": []
    },
    "processNameFilter": {
      "enabled": false,
      "include": true,
      "data": []
    }
  }
}